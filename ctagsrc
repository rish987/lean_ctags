--regex-lean=/\<class[ \t]\+\([A-Za-z0-9_]\+\)[^\.]/\1/t,Test,Test/b
--regex-lean=/\<class[ \t]\+{[^}]*}[ \t]\+\([A-Za-z0-9_]\+\)[^\.]/\1/t,Test,Test/b
