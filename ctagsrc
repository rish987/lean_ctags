--regex-lean=/\<class[ \t]\+\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/X,class,class/b
--regex-lean=/\<class[ \t]\+{[^}]*}[ \t]\+\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/X,class,class/b
--regex-lean=/\<class[ \t]\+[A-Za-z0-9_\.]\+\.\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/X,class,class/b
--regex-lean=/\<class[ \t]\+{[^}]*}[ \t]\+[A-Za-z0-9_\.]\+\.\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/X,class,class/b
--regex-lean=/\<instance[ \t]\+\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/x,instance,instance/b
--regex-lean=/\<instance[ \t]\+{[^}]*}[ \t]\+\([A-Za-z0-9_]\+\)[^A-Za-z0-9_\.]/\1/x,instance,instance/b
